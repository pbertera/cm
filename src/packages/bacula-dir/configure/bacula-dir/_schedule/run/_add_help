#!/bin/bash
#@============================================
#@cm bacula-dir schedule <schedule-id> run add
#@============================================
#@
#@|syntax| 
#@
#@|normal-command| cm bacula-dir schedule <schedule-id> run add <name>
#@
#@|negate-command| nocm bacula-dir schedule <schedule-id> run add <name>
#@
#@|configuration-mode|
#@
#@|parameters| 
#@
#@* `run-id`:  is the identifier of run to add to `schedule-id`
#@
#@|default| `none`
#@
#@|usage|
#@
#@Questo comando aggiunge un nuovo run allo schedule `schedule-id`
#@
#@Una volta aggiunto il run sar√† possiible configurarlo con i comandi
#@
#@ *cm bacula-dir schedule <schedule-id> run <run-id> \**
#@



. ${__cm_lib_dir}/cmlib

__cm_print_help_header $@

echo "add run to bacula schedule"
echo 
echo "Parameter: <run-id> The run id"

__cm_print_help_bottom
